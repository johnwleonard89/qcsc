---
import CitySpecificServiceTemplate from '../../templates/CitySpecificServiceTemplate.astro';
import { servicesConfig } from '../../data/servicesData.js';

const service = servicesConfig.industrial.services.find(s => s.title === "Pressure Washing & Protection");

const cityData = {
  cityName: 'Cincinnati',
  state: 'Ohio', 
  county: 'Hamilton',
  service: service,
  seo: {
    title: 'Cincinnati Industrial Pressure Washing & Protection | Professional Industrial Concrete Cleaning | Queen City Surface Coatings',
    description: 'Professional industrial concrete pressure washing and protective services in Cincinnati, Ohio. High-pressure cleaning, chemical stain removal, and protective sealer application for industrial properties.',
    keywords: 'Cincinnati industrial pressure washing, industrial concrete cleaning, protective sealer application, chemical stain removal, industrial concrete maintenance'
  },
  serviceArea: {
    description: 'Queen City Surface Coatings provides professional industrial pressure washing and protection services throughout Cincinnati, Ohio.',
    neighborhoods: [
      'Hyde Park', 'Clifton', 'Mount Adams', 'Oakley', 'East Walnut Hills',
      'Walnut Hills', 'Columbia-Tusculum', 'Pleasant Ridge', 'Madisonville',
      'Over-the-Rhine', 'Downtown', 'West End', 'Price Hill', 'Westwood', 'Northside'
    ]
  }
};
---

<CitySpecificServiceTemplate 
  cityName={cityData.cityName}
  state={cityData.state}
  county={cityData.county}
  service={cityData.service}
  seo={cityData.seo}
  serviceArea={cityData.serviceArea}
/>